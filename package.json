{
  "name": "shift-builder",
  "author": "Nick Wittwer",
  "version": "1.0.0",
  "description": "A tool for previewing responsive sites.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:nwittwer/shift.git"
  },
  "homepage": "https://shift.nickwittwer.com",
  "scripts": {
    "start": "kill -9 $(lsof -i TCP:8000 | grep LISTEN | awk '{print $2}') & gulp & nw dist",
    "build": "gulp app",
    "deploy": "gulp release",
    "tidy": "standard 'src/js/features/**/*.js'",
    "vue-serve": "vue-cli-service serve",
    "vue-build": "vue-cli-service build",
    "vue-lint": "vue-cli-service lint"
  },
  "bugs": {
    "url": "https://github.com/nwittwer/shift/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "archiver": "^3.0.0",
    "autoprefixer": "^7.1.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "dotenv": "^6.0.0",
    "express": "^4.15.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-github-release": "^1.2.1",
    "gulp-handlebars": "^5.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-wrap": "^0.14.0",
    "gulp-zip": "^4.2.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "inquirer": "^6.2.0",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.3",
    "nw": "^0.33.1-sdk",
    "ora": "^1.2.0",
    "path": "^0.12.7",
    "require-dir": "^1.0.0",
    "sass-loader": "^6.0.5",
    "standard": "^12.0.1",
    "undertaker-forward-reference": "^1.0.2",
    "url-loader": "^0.5.8",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {
    "panzoom": "^6.3.2",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
