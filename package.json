{
  "name": "shift-builder",
  "author": "Nick Wittwer",
  "version": "1.0.0",
  "description": "A tool for previewing responsive sites.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:nwittwer/shift.git"
  },
  "homepage": "https://shift.nickwittwer.com",
  "scripts": {
    "start": "lsof -ti:8080 | xargs kill && vue-cli-service serve & gulp & nw dist",
    "build": "gulp app",
    "deploy": "gulp release",
    "test": "jest",
    "lint": "vue-cli-service lint"
  },
  "bugs": {
    "url": "https://github.com/nwittwer/shift/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.25",
    "archiver": "^3.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chalk": "^1.1.3",
    "dotenv": "^6.0.0",
    "eslint": "^5.8.0",
    "express": "^4.15.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-github-release": "^1.2.1",
    "gulp-handlebars": "^5.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-wrap": "^0.14.0",
    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "json-format": "^1.0.1",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "nw": "^0.35.4-sdk",
    "opn": "^5.4.0",
    "ora": "^1.2.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.12.1",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.0",
    "undertaker-forward-reference": "^1.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {
    "jest-vue-preprocessor": "^1.4.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.5.17",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
