{
  "name": "screens",
  "version": "0.0.0-alpha-1",
  "node-main": "server.js",
  "main": "http://localhost:8000",
  "node-remote": "<all_urls>",
  "icon": "icon.icns",
  "window": {
    "height": 750,
    "width": 1200,
    "min_width": 400,
    "min_height": 400,
    "frame": false,
    "transparent": true,
    "show": false
  },
  "dependencies": {
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "html": "^1.0.0",
    "http": "0.0.0",
    "jsdom": "^11.5.1",
    "node-cmd": "^3.0.0",
    "node-php-server": "^0.1.2",
    "path": "^0.12.7",
    "request": "^2.83.0",
    "require": "^2.4.20"
  },
  "chromium-args": "--enable-logging=stderr --show-fps-counter --enable-experimental-web-platform-features --remote-debugging-port=9999",
  "webview": {
    "partitions": [{
      "name": "trusted",
      "accessible_resources": [
        "storage",
        "tabs",
        "activeTab",
        "<all_urls>",
        "*://*/*"
      ]
    }]
  }
}